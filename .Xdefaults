URxvt*font:              xft:DejaVu\ Sans\ Mono:pixelsize=13
! URxvt*font:                -*-dina-medium-r-normal-*-*-*-*-*-*-*-*-*


URxvt*saveLines:         32767
URxvt*scrollBar:         false
URxvt*termName:          rxvt
URxvt*cutchars:          "()*,<>[]{}|"
URxvt*secondaryScroll:   true
URxvt*visualBell:        true
URxvt*background:        #000000
URxvt*foreground:        #FFFFFF
URxvt*cursorColor:       #FFAF00

URxvt.keysym.M-u: perl:mark-yank-urls:activate_mark_mode
URxvt.underlineURLs: true
URxvt.perl-lib: /home/saku/.urxvt
URxvt.perl-ext: selection,mark-yank-urls
URxvt.urlLauncher: conkeror
urxvt*perl-ext-common:   default,matcher
URxvt*matcher.button:    1
URxvt*matcher.pattern.1: \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]

xterm*utf8:              2
xterm*background:        #1c1c1c

Xft.lcdfilter:           lcddefault
Xft.hintstyle:           hintslight

Emacs.font:              Monospace-9
emacs.FontBackend:       xft

