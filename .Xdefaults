!URxvt*font:                     xft:Consolas:pixelsize=12
!URxvt*boldFont:                 xft:Consolas:bold
URxvt*font:                    -*-dina-medium-r-normal-*-*-*-*-*-*-*-*-*
URxvt*boldFont:                -*-dina-bold-r-normal-*-*-*-*-*-*-*-*-*

URxvt*saveLines:                32767
URxvt*scrollBar:	            false
URxvt*tintColor:                #000000
URxvt*shading:                  30
URxvt*termName:		            rxvt
URxvt*inheritPixmap:	        true
URxvt*cutchars:		            "()*,<>[]{}|"
URxvt*secondaryScroll:	        true
urxvt*perl-lib:		            /usr/lib/urxvt/perl/
urxvt*perl-ext-common:	        default,matcher
URxvt*urlLauncher:	            /usr/bin/firefox
URxvt*matcher.button:	        1
URxvt*matcher.pattern.1:        \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt*visualBell:	            true
URxvt.depth: 					24
xterm*utf8:                     2


xterm*background:                    #1c1c1c
*foreground:                    #B4B4B4

!--[Colours]--! 
URxvt*cursorColor: #FFAF00

! tangoesque scheme
! Black
*color0:        #000000
*color8:        #555753
! Red
*color1:        #ff6565
*color9:        #ff8d8d
! Green
*color2:        #93d44f
*color10:       #c8e7a8
! Yellow
*color3:        #eab93d
*color11:       #ffc123
! Blue
*color4:        #204a87
*color12:       #3465a4
! Mangenta
*color5:        #ce5c00
*color13:       #f57900
! Cyan 
*color6:        #89b6e2
*color14:       #46a4ff
! White
*color7:        #cccccc
*color15:       #ffffff

