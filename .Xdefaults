URxvt*font:               xft:Droid Sans Mono:pixelsize=16:hintstyle=slight

URxvt*saveLines:          32767
URxvt*scrollBar:          false
URxvt*termName:           rxvt
URxvt*cutchars:           "()*,<>[]{}|"
URxvt*secondaryScroll:    true
URxvt*visualBell:         true
URxvt*background:         #000000
URxvt*foreground:         #FFFFFF
URxvt*cursorColor:        #FFFFFF

URxvt.keysym.M-u:         perl:mark-yank-urls:activate_mark_mode
URxvt.underlineURLs:      true
URxvt.perl-lib:           /home/saku/.urxvt
URxvt.perl-ext:           selection,mark-yank-urls
URxvt.urlLauncher:        firefox
urxvt*perl-ext-common:    default,matcher
URxvt*matcher.button:     1
URxvt*matcher.pattern.1:  \\bi((https?)://[-\\w]+(\\.\\w[-\\w]*)+|(?i:[a-z0-9](?:[-a-z0-9]*[a-z0-9])?\\.)+(?-i:com\\b|edu\\b|biz\\b|gov\\b|in(?:t|fo)\\b|mil\\b|net\\b|org\\b|[a-z][a-z]\\.[a-z][a-z]\\b))(:\\d+)?/[^.!,?;"\\'<>()\[\]\{\}\s\x7F-\\xFF]*([.!,?]+[^.!,?;"\\'<>()\\[\\]\{\\}\s\\x7F-\\xFF]+)*)?
! URxvt*matcher.pattern.1:  \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]

xterm*utf8:               2

Xft.hintstyle:            hintnone
Xft.lcdfilter:            lcddefault

Emacs.font:               Monospace-10

